{
    "name": "vscodemq2",
    "publisher": "DeWayneJohnsonbaugh",
    "version": "0.0.5",
    "displayName": "vscodemq2",
    "description": "Macroquest2 Macro Language VSCode Extension",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "mq2macro",
                "aliases": [
                    "mq2macro",
                    "mq2macro"
                ],
                "extensions": [
                    ".mac",
                    ".inc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mq2macro",
                "scopeName": "source.mq2macro",
                "path": "./syntaxes/mq2macro.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "mq2macro",
                "path": "./snippets/mq2macro.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/djohnsonbaugh/vscodemq2macro.git"
    },
    "keywords": ["everquest", "EQ", "Macroquest", "Macro", "MQ2", ".mac", "Macroquest2"]
}
